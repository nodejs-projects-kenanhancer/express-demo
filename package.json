{
  "name": "express-demo",
  "version": "1.0.0",
  "description": "Express web framework demo for Node.js",
  "main": "index.js",
  "scripts": {
    "start:mongodb": "docker run -p 27017:27017 --name mongoDbTest -d mongo:latest",
    "test": "npm run test:unit",
    "start": "node src/app",
    "start:srcV2": "node srcV2/app",
    "start:srcV3": "node srcV3/app",
    "start:srcV4": "node srcV4/app",
    "start:srcV5": "node srcV5/app",
    "build:src": "npx babel src --out-dir dist --source-maps --delete-dir-on-start",
    "build:srcV2": "npx babel srcV2 --out-dir distV2 --source-maps --delete-dir-on-start",
    "watch": "nodemon src/app",
    "watch-babel:src": "nodemon --exec babel-node src/app",
    "watch-babel:srcV2": "nodemon --exec babel-node srcV2/app",
    "watch-babel:srcV3": "nodemon --exec babel-node srcV3/app",
    "watch-babel:srcV4": "nodemon --exec babel-node srcV4/app",
    "watch-babel:srcV5": "nodemon --exec babel-node srcV5/app",
    "test:unit": "mocha",
    "test:unit:watch": "mocha --watch",
    "test:unit:spec:style": "mocha --reporter spec",
    "test:unit:dot:style": "mocha --reporter dot",
    "test:unit:nyan:style": "mocha --reporter nyan",
    "test:unit:tap:style": "mocha --reporter tap",
    "test:unit:landing:style": "mocha --reporter landing",
    "test:unit:list:style": "mocha --reporter list",
    "test:unit:progress:style": "mocha --reporter progress",
    "test:unit:json:style": "mocha --reporter json",
    "test:unit:json-stream:style": "mocha --reporter json-stream",
    "test:unit:min:style": "mocha --reporter min",
    "test:unit:mochawesome:style": "mocha --reporter mochawesome --reporter-option reportDir=./test/reports/mochawesome-report",
    "test:unit:xunit:style": "mocha --reporter xunit --reporter-option output=./test/reports/test-xunit.xml",
    "test:unit:markdown:style": "mocha --reporter markdown | cat ./test/reports/docs/head.html - ./test/reports/docs/tail.html > ./test/reports/docs/test.html",
    "test:unit:doc:style": "mocha --reporter=doc | cat docs/head.html - docs/tail.html > docs/test.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodejs-projects-kenanhancer/express-demo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nodejs-projects-kenanhancer/express-demo/issues"
  },
  "homepage": "https://github.com/nodejs-projects-kenanhancer/express-demo#readme",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "mongoose": "^5.9.18",
    "morgan": "^1.10.0",
    "pug": "^2.0.4",
    "redis": "^3.0.2",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/plugin-transform-typescript": "^7.10.1",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.10.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^7.2.0",
    "mochawesome": "^6.1.1",
    "nodemon": "^2.0.4"
  }
}
